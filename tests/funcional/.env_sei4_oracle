##### Diretório em que está localizada a instalação do SEI #####
SEI_PATH=/home/XXXXXXXXXXXXXX/dev/sei4
ENVIRONMENT_VERSION=9.8

SEI_CONFIG=./assets/config/ConfiguracaoSEI4.php
SEI_DATABASE_NAME=sei
SEI_DATABASE_USER=sei
SEI_DATABASE_PASSWORD=sei_user
SEI_DATABASE_USER_SCRIPT=sei
SEI_DATABASE_PASSWORD_SCRIPT=sei_user

SIP_CONFIG=./assets/config/ConfiguracaoSip4.php
SIP_DATABASE_NAME=sip
SIP_DATABASE_USER=sip
SIP_DATABASE_PASSWORD=sip_user
SIP_DATABASE_USER_SCRIPT=sip
SIP_DATABASE_PASSWORD_SCRIPT=sip_user



############### CONFIGURAÇÕES GERAIS DO ÓRGÃO 01 ###############
ORG1_HOSTNAME=org1-http
ORG1_PORT=8000
ORG1_SIGLA_ORGAO=ABC
ORG1_APP_SIP_CHAVE_ACESSO=XXXXXXXXXXXXXX
ORG1_APP_SEI_CHAVE_ACESSO=XXXXXXXXXXXXXX
ORG1_CERTIFICADO=/opt/sei/config/mod-pen/certificado_org1.pem
ORG1_CERTIFICADO_SENHA=XXXXXXXXXXXXXX

##### BANCO DE DADOS DO ÓRGÃO 1 #####
# Configurações do banco de dados ORACLE do ÓRGÃO 1
ORG1_DATABASE_IMAGE=processoeletronico/vagrant_sei4_oracle
ORG1_DATABASE_HOST=oracle
ORG1_DATABASE_TYPE=Oracle
ORG1_DATABASE_GUEST_PORT=1521
ORG1_DATABASE_HOST_PORT=15211
ORG1_DATABASE_DATA_DIR=/opt/oracle/oradata
CONTEXTO_ORGAO_A_DB_SEI_DSN =oci:dbname=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=org1-database)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=XE)))




############### CONFIGURAÇÕES GERAIS DO ÓRGÃO 02 ###############

ORG2_HOSTNAME=org2-http
ORG2_PORT=8000
ORG2_SIGLA_ORGAO=ABC
ORG2_APP_SIP_CHAVE_ACESSO=XXXXXXXXXXXXXX
ORG2_APP_SEI_CHAVE_ACESSO=XXXXXXXXXXXXXX
ORG2_CERTIFICADO=/opt/sei/config/mod-pen/certificado_org2.pem
ORG2_CERTIFICADO_SENHA=XXXXXXXXXXXXXX

# Configurações do banco de dados ORACLE do ÓRGÃO 2
ORG2_DATABASE_IMAGE=processoeletronico/vagrant_sei4_oracle
ORG2_DATABASE_HOST=oracle
ORG2_DATABASE_TYPE=Oracle
ORG2_DATABASE_GUEST_PORT=1521
ORG2_DATABASE_HOST_PORT=15212
ORG2_DATABASE_DATA_DIR=/opt/oracle/oradata
CONTEXTO_ORGAO_B_DB_SEI_DSN =oci:dbname=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=org2-database)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=XE)))




